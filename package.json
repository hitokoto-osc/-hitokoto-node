    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jest-config": "26.4.2",
    "jest-pnp-resolver": "1.2.2",
    "lint-staged": "10.5.4",
    "prettier": "2.1.1",
