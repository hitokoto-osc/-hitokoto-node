{
  "name": "@hitokoto/node",
  "version": "1.0.0",
  "description": "hitokoto.cn official node.js sdk.",
  "main": "build/core.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "yarn run lint-eslint",
    "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js {src,scripts}/**/*.{ts,tsx,js}",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitokoto-osc/node.git"
  },
  "keywords": [
    "sdk",
    "node.js",
    "hitokoto"
  ],
  "author": "a632079 <a632079@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hitokoto-osc/node/issues"
  },
  "homepage": "https://github.com/hitokoto-osc/node#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/form-data": "^2.5.0",
    "@types/got": "^9.6.9",
    "@types/jest": "^25.1.3",
    "@types/query-string": "^6.3.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-config": "^25.1.0",
    "jest-pnp-resolver": "^1.2.1",
    "lint-staged": "^10.0.7",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "installConfig": {
    "pnp": true
  },
  "dependencies": {
    "@yarnpkg/pnpify": "^2.0.0-rc.18",
    "form-data": "^3.0.0",
    "got": "^10.6.0",
    "query-string": "^6.11.0"
  }
}
