{
  "name": "@hitokoto/node-sdk",
  "version": "1.0.4",
  "description": "hitokoto.cn official node.js sdk.",
  "main": "dist/core.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "yarn run lint-eslint",
    "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js {src,scripts}/**/*.{ts,tsx,js}",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitokoto-osc/node.git"
  },
  "keywords": [
    "sdk",
    "node.js",
    "hitokoto"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": "a632079 <a632079@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hitokoto-osc/node/issues"
  },
  "homepage": "https://github.com/hitokoto-osc/node#readme",
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/form-data": "2.5.0",
    "@types/jest": "26.0.13",
    "@types/node": "13.13.15",
    "@types/node-fetch": "2.5.7",
    "@types/query-string": "6.3.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "@yarnpkg/pnpify": "2.1.0",
    "conventional-changelog-cli": "2.0.34",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-html": "6.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.5",
    "jest": "25.5.4",
    "jest-config": "25.5.4",
    "jest-pnp-resolver": "1.2.2",
    "lint-staged": "10.2.11",
    "ts-jest": "25.5.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "installConfig": {
    "pnp": true
  },
  "dependencies": {
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.0",
    "query-string": "^6.11.0"
  }
}
